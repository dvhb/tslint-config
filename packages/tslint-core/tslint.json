{
  "extends": ["tslint-config-airbnb", "tslint-config-prettier", "tslint-plugin-prettier"],
  "rules": {
    "import-name": false,
    "no-console": [true, "log"],
    "no-default-export": true,
    "prettier": true,
    "variable-name": [true, "check-format", "allow-pascal-case", "allow-leading-underscore"],
    "max-file-line-count": [true, 300],
    "max-line-length": [true, 120],
    "function-name": [ true, { "method-regex": "^[a-z][\\w\\d]+$", "private-method-regex": "^[a-z][\\w\\d]+$", "protected-method-regex": "^[a-z][\\w\\d]+$", "static-method-regex": "^[a-z][\\w\\d]+$", "function-regex": "^[a-zA-Z][\\w\\d]+$" } ]
  }
}
